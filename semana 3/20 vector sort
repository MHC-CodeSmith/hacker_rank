#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    int N;
    cin >> N;                     

    vector<int> v(N);             
    for (int i = 0; i < N; ++i)   
        cin >> v[i];

    sort(v.begin(), v.end());     

    for (int x : v)               
        cout << x << ' ';

    return 0;
}
